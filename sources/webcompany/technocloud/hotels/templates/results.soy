                                                                <td class="{css stars}"></td>
                                                                <td class="{css stars}"></td>
                                                                <td class="{css stars}"></td>
                                                                <td></td>
                                                                <td></td>
                                                                <td>
                                                                    <input id="ucResultadoHotel_chkCategoria_4" type="radio" name="ucResultadoHotel$chkCategoria" value="4" onclick="    javascript: setTimeout('__doPostBack(\'ucResultadoHotel$chkCategoria$4\',\'\')', 0)"><label for="ucResultadoHotel_chkCategoria_4"></label></td>
