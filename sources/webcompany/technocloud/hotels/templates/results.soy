        </div>
    </div>
    <div style="clear: both;"></div>
</div>
{/template}

/**
* @param filters The filters.
* dibuja los filtros
*/
{template .filters}
<div>
    <div>
        <a class="{css btnreturnsearch} {css background-color} {css border-color} {css text-color}" href="javascript:void(0);">Volver a buscar</a>
    </div>
    <div class="{css search}">
    </div>
    <div class="{css filters}">
        <div class="{css ux-common-filters} {css ux-flights-filters} {css border-color}" style="">
            <div style="display: none;" class="{css ux-common-filter-section} {css ux-flights-filter-airlines} {css ux-flights-order}">
                <div class="{css order}" style="">
                    <form class="{css order-form}">
                        <ul>
                            <li class="{css orderby}">
                                <label for="orderby">
                                    <span>Ordenar por</span>
                                    <select name="orderby" id="orderby" class="{css flights-select}">
                                        <option value="FARE_ASCENDING">Precio </option>
                                        <option value="STOPSCOUNT_ASCENDING">Escalas </option>
                                        <option value="TOTALFARE_ASCENDING" selected="">Precio final </option>
                                        <option value="DURATION_ASCENDING">Duración </option>
                                        <option value="PERSONAL_ASCENDING">Más Elegidos </option>
                                    </select>
                                </label>
                            </li>
                        </ul>
                    </form>
                </div>
            </div>
            <div class="{css ux-common-filter-section} {css ux-flights-filter-airlines}">
                <div class="{css ux-common-filter-header}">
                    <h4>Aerolíneas</h4>
                    <span class="{css ux-common-filter-toggle}"></span>
                </div>
                <div class="{css ux-common-filter-body}">
                    <ul class="{css ux-common-filter-options} {css items} {css accordion-content}">
                        <li class="{css item} {css airlines-all}" style="">
                            <label for="airlines-all" class="{css ux-common-filter-all} {css ux-common-filter-disabled} {css selected} {css ux-common-filter-selected}">
                                <input id="airlines-all" type="checkbox" disabled name="airlines" value="*" checked="" class="{css ux-common-filter-disabled}">
                                <span class="{css label}">Todas las aerolíneas</span>
                                <span class="{css total} {css ux-common-filter-total} {css background-color} {css text-color}">{length($filters.airLines)}</span>
                            </label>
                        </li>

                        {foreach $airLine in $filters.airLines}
                           
                        <li class="{css item} {css airlines-LA}" style="">
                            <label for="airlines-{$airLine.value}" class=" ">
                                <input id="airlines-{$airLine.value}" type="checkbox" name="airlines" value="{$airLine.value}" enabled="" class="{$airLine}">
                                <img src="http://search.technocloud.com.co/static/images/airline/{$airLine.value}.gif" title="Lan" onerror="this.src='http://search.technocloud.com.co/static/images/airline/00.gif'" alt="{$airLine.value}">
                                <span class="{css label}"></span>
                                <span class="{css total} {css ux-common-filter-suboptions} {css ux-common-filter-total}">{$airLine.count}</span>
                            </label>
                        </li>
                        {/foreach}

                   
                    </ul>
                    <div style="display: none;" class="{css unique-airline}">
                        <div class="{css ux-common-filter-section} {css ux-flights-filter-sameairlines}">
                            <div class="{css ux-common-filter-body}">
                                <ul class="{css ux-common-filter-options} {css items} {css accordion-content}">
                                    <li class="{css item}">
                                        <label for="unique-airline" class="{css enabled}">
                                            <input id="unique-airline" type="checkbox" name="unique-airline" value="TRUE"><span>Ida y vuelta por la misma aerolínea</span></label>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="{css ux-common-filter-section} {css ux-flights-filter-airlines}">
                <div class="{css ux-common-filter-header}">
                    <h4>Precios</h4>
                    <span class="{css ux-common-filter-toggle}"></span>
                </div>
                <div class="{css ux-common-filter-body}">
                    <ul class="{css ux-common-filter-options} {css items} {css accordion-content}">
                        <li class="{css item} {css alliances-all}" style="">
                            <label for="alliances-all" class="{css ux-common-filter-all} {css ux-common-filter-disabled} {css selected} {css ux-common-filter-selected}">
                                <input id="alliances-all" type="checkbox" name="amountCurrencys" value="*" checked="" disabled="" class="{css ux-common-filter-disabled}">
                                <span class="{css label}">Todos los precios</span>
                                <span class="{css total} {css ux-common-filter-total} {css background-color} {css text-color}">{length($filters.amountCurrencys)}</span></label>
                        </li>
                        {foreach $amountCurrency in $filters.amountCurrencys}
                           
                        <li class="{css item} {css alliances-ST}" style="">
                            <label for="alliances-{$amountCurrency.value}" class="">
                                <input id="alliances-{$amountCurrency.value}" type="checkbox" name="amountCurrencys" value="{$amountCurrency.value}" enabled="" class="{css ST}">
                                <span class="{css label}">{$amountCurrency.value}</span><span class="{css total} {css ux-common-filter-suboptions} {css ux-common-filter-total}">{$amountCurrency.count}</span>
                            </label>
                        </li>
                        {/foreach}
                   
                    </ul>
                </div>
            </div>
            <div class="{css ux-common-filter-section} {css ux-flights-filter-stops}">
                <div class="{css ux-common-filter-header}">
                    <h4>Escalas</h4>
                    <span class="{css ux-common-filter-toggle}"></span>
                </div>
                <div class="{css ux-common-filter-body}">
                    <ul class="{css ux-common-filter-options} {css items} {css accordion-content}">
                        <li class="{css item} {css stops-all}" style="">
                            <label for="stops-all" class="{css ux-common-filter-all} {css ux-common-filter-disabled selected} {css ux-common-filter-selected}">
                                <input id="stops-all" type="checkbox" name="stops" value="*" checked="" disabled="" class="{css ux-common-filter-disabled}">
                                <span class="{css label}">Todas las escalas</span>
                                <span class="{css total} {css ux-common-filter-total} {css background-color} {css text-color}">{length($filters.stops)}</span></label>
                        </li>
                        {foreach $stop in $filters.stops}
                           
                        <li class="{css item} {css stops}-{$stop.value}" style="">
                            <label for="stops-{$stop.value}" class="{css  ux-common-filter-disabled}">
                                <input id="stops-{$stop.value}" type="checkbox" name="stops" value="{$stop.value}" class="{css NONE} {css ux-common-filter-disabled}">
                                <span class="{css label}">{$stop.value}</span><span class="{css total} {css ux-common-filter-suboptions} {css ux-common-filter-total}">{$stop.count}</span></label>
                        </li>
                        {/foreach}
                   
                    </ul>
                </div>
            </div>
            <div class="{css ux-common-filter-section} {css ux-flights-filter-originairports}">
                <div class="{css ux-common-filter-header}">
                    <h4>Aeropuertos de origen</h4>
                    <span class="{css ux-common-filter-toggle}"></span>
                </div>
                <div class="{css ux-common-filter-body}">
                    <ul class="{css ux-common-filter-options} {css items} {css accordion-content}">
                        <li class="{css item} {css airport-outbound-all}" style="">
                            <label for="airport-outbound-all" class="{css ux-common-filter-all} {css ux-common-filter-disable} {css selected} {css ux-common-filter-selected}">
                                <input id="airport-outbound-all" type="checkbox" name="departureAirports" value="*" checked="" class="{css ux-common-filter-disabled}">
                                <span class="{css label}">Todos los aeropuertos</span>
                                <span class="{css total} {css ux-common-filter-total} {css background-color} {css text-color}">{length($filters.departureAirports)}</span>
                            </label>
                        </li>

                        {foreach $departureAirport in $filters.departureAirports}
                           
                        <li class="{css item} {css airport-outbound}" style="">
                            <label for="airport-outbound-{$departureAirport.id}" class="">
                                <input id="airport-outbound-{$departureAirport.id}" type="checkbox" name="departureAirports" value="{$departureAirport.id}" enabled="" class="{css BOG}">
                                <span class="{css label}">{$departureAirport.label}</span>
                                <span class="{css total} {css ux-common-filter-suboptions} {css ux-common-filter-total}">{$departureAirport.count}</span>
                            </label>
                        </li>
                        {/foreach}
                   
                    </ul>
                    <div style="display: none;" class="{css unique-airport}">
                        <div class="{css ux-common-filter-section} {css ux-flights-filter-sameairports}">
                            <div class="{css ux-common-filter-body}">
                                <ul class="{css ux-common-filter-options} {css items} {css accordion-content}">
                                    <li class="{css item}">
                                        <label for="unique-airport" class="{css enabled}">
                                            <input id="unique-airport" type="checkbox" name="departureAirports" value="TRUE">
                                            <span>Partida y regreso por el mismo aeropuerto</span>
                                        </label>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="{css ux-common-filter-section} {css ux-flights-filter-destinyairports}">
                <div class="{css ux-common-filter-header}">
                    <h4>Aeropuertos de destino</h4>
                    <span class="{css ux-common-filter-toggle}"></span>
                </div>
                <div class="{css ux-common-filter-body}">
                    <ul class="{css ux-common-filter-options} {css items} {css accordion-content}">
                        <li class="{css item} {css airport-inbound-all}" style="">
                            <label for="airport-inbound-all" class="{css ux-common-filter-all} {css ux-common-filter-disabled} {css selected} {css ux-common-filter-selected}">
                                <input id="airport-inbound-all" type="checkbox" name="arrivalAirPorts" value="*" checked="" disabled="" class="{css ux-common-filter-disabled}">
                                <span class="{css label}">Todos los aeropuertos</span>
                                <span class="{css total} {css ux-common-filter-total} {css background-color} {css text-color}">{length($filters.arrivalAirPorts)}</span></label>
                        </li>
                        {foreach $arrivalAirPort in $filters.arrivalAirPorts}
                           
                        <li class="{css item} {css airport-inbound-BSB}" style="">
                            <label for="airport-inbound-{$arrivalAirPort.id}" class="">
                                <input id="airport-inbound-{$arrivalAirPort.id}" type="checkbox" name="arrivalAirPorts" value="{$arrivalAirPort.id}" enabled="" class="{css BSB}">
                                <span class="{css label}">{$arrivalAirPort.label}</span>
                                <span class="{css total} {css ux-common-filter-suboptions} {css ux-common-filter-total}">{$arrivalAirPort.count}</span>
                            </label>
                        </li>
                        {/foreach}
                   
                    </ul>
                </div>
            </div>
            <div class="{css ux-common-filter-section} {css ux-flights-filter-airlines}">
                <div class="{css ux-common-filter-header}">
                    <h4>Hora llegada</h4>
                    <span class="{css ux-common-filter-toggle}"></span>
                </div>
                <div class="{css ux-common-filter-body}">
                    <ul class="{css ux-common-filter-options} {css items} {css accordion-content}">
                        <li class="{css item} {css alliances-all}" style="">
                            <label for="arrivalTime-all" class="{css ux-common-filter-all} {css ux-common-filter-disabled} {css selected} {css ux-common-filter-selected}">
                                <input id="arrivalTime-all" type="checkbox" name="arrivalTimes" value="*" checked="" disabled="" class="{css ux-common-filter-disabled}">
                                <span class="{css label}">Todas las horas</span>
                                <span class="{css total} {css ux-common-filter-total} {css background-color} {css text-color}">{length($filters.arrivalTimes)}</span>
                            </label>
                        </li>
                        {foreach $arrivalTime in $filters.arrivalTimes}
                           
                        <li class="{css item} {css alliances-ST}" style="">
                            <label for="arrivalTime-{$arrivalTime.value}" class="">
                                <input id="arrivalTime-{$arrivalTime.value}" type="checkbox" name="arrivalTimes" value="{$arrivalTime.value}" enabled="" class="{css ST}">
                                <span class="{css label}">{$arrivalTime.value}</span><span class="{css total} {css ux-common-filter-suboptions} {css ux-common-filter-total}">{$arrivalTime.count}</span>
